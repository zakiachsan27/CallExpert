        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "c:\\Projects\\CallExpert"
      ],
      "env": {}
    }
  }
}
